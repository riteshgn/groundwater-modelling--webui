        "build:dev:watch": "better-npm-run build:dev:watch",
        "prebuild": "npm install"
    },
    "betterScripts": {
        "build": {
            "command": "webpack --config webpack.config.js --mode=production",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "build:dev": {
            "command": "webpack --config webpack.config.js --mode=development",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "build:dev:watch": {
            "command": "webpack --config webpack.config.js --watch --mode=development",
            "env": {
                "NODE_ENV": "development"
            }
        }
    },
